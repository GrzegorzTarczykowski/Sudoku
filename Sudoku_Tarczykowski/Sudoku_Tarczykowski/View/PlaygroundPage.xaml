        <Button x:Name="buttonHelp" Command="{Binding ButtonHelpClickCommand}" Grid.Row="5" Grid.Column="9" Height="60" Width="Auto" Content=" Podpowiedź " FontSize="25" FontWeight="Bold"/>
        <TextBox x:Name="chosenTextBoxToHelp" Grid.Row="5" Grid.Column="9" Width="1" Height="1" FontSize="1" HorizontalContentAlignment="Center" Text="{Binding ChosenTextBoxToHelp, UpdateSourceTrigger=PropertyChanged}"/>
        <Label x:Name="amountOfHelpLabel" Grid.Row="6" Grid.Column="9" Height="60" Width="Auto" Content="{Binding AmountOfHelp}" FontSize="25" FontWeight="Bold" HorizontalAlignment="Center"/>
        <Button x:Name="buttonSolveGame" Command="{Binding ButtonSolveGameClickCommand}" Grid.Row="7" Grid.Column="9" Height="60" Width="Auto" Content=" Rozwiąż " FontSize="25" FontWeight="Bold"/>
        <Button x:Name="buttonPrintGame" Command="{Binding ButtonPrintGameClickCommand}" Grid.Row="8" Grid.Column="9" Height="60" Width="Auto" Content=" Drukuj " FontSize="25" FontWeight="Bold"/>
        <ComboBox ItemsSource="{Binding LevelOfTheGame}" SelectedItem="{Binding SelectedLevel}" Grid.Row="1" Grid.Column="9" Width="Auto" FontSize="20" FontWeight="Bold"/>
    </Grid>
</Page>
